{
  "name": "lightning-inspector",
  "version": "1.0.9",
  "description": "The Salesforce Lightning Inspector",
  "main": "Gruntfile.js",
  "repository": "git@git.soma.salesforce.com:aura/lightning-inspector.git",
  "author": "Kris <kgray@salesforce.com>",
  "license": "MIT",
  "private": true,
  "jest": {
    "setupFiles": [
      "raf/polyfill"
    ],
    "transformIgnorePatterns": [
      "node_modules/?!(react-lds)"
    ]
  },
  "scripts": {
    "build": "export NODE_ENV=production; export GIT_BRANCH=`git rev-parse --abbrev-ref HEAD`; yarn build:js;",
    "build:aura": "mvn clean install",
    "build:js": "./node_modules/grunt/bin/grunt build",
    "build:readme": "./node_modules/doctoc/doctoc.js ./*.md",
    "bump": "./bin/bump.js",
    "doc": "./node_modules/.bin/esdoc",
    "start": "bin/jetty-run.sh",
    "postinstall": "exit 0; git submodule init; git submodule update;",
    "test": "jest --watchAll --coverage",
    "test:prod": "jest --coverage",
    "watch:dashboard": "export NODE_ENV=development; export GIT_BRANCH=`git rev-parse --abbrev-ref HEAD`;  webpack-dashboard -- ./node_modules/grunt/bin/grunt watch",
    "watch": "export NODE_ENV=development; export GIT_BRANCH=`git rev-parse --abbrev-ref HEAD`; ./node_modules/grunt/bin/grunt watch",
    "watch:prod": "export NODE_ENV=production; export GIT_BRANCH=`git rev-parse --abbrev-ref HEAD`;  ./node_modules/grunt/bin/grunt watch"
  },
  "devDependencies": {
    "@salesforce-ux/design-system": "2.5.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-async-to-module-method": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "crx": "^3.2.1",
    "css-loader": "^0.28.7",
    "doctoc": "^1.3.0",
    "esdoc": "^1.0.4",
    "esdoc-jsx-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "file-loader": "^1.1.5",
    "filemanager-webpack-plugin": "^1.0.26",
    "grunt": "^1.0.1",
    "grunt-babel": "^7.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-uglify-es": "^3.3.0",
    "grunt-stripcomments": "^0.7.2",
    "grunt-webpack": "^3.0.2",
    "grunt-zip": "^0.17.1",
    "jest": "^23.1.0",
    "json-bump": "^0.1.3",
    "json-loader": "^0.5.7",
    "less": "^3.0.4",
    "less-loader": "^4.0.5",
    "load-grunt-tasks": "^4.0.0",
    "node-sass": "^4.5.3",
    "raf": "^3.4.0",
    "raw-loader": "^0.5.1",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^7.0.3",
    "strip-json-comments": "^2.0.1",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "web-ext": "^2.7.0",
    "webpack": "^4.5.0",
    "webpack-dashboard": "^2.0.0",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2",
    "worker-loader": "^2.0.0"
  },
  "dependencies": {
    "@salesforce-ux/design-system": "^2.5.2",
    "@salesforce/design-system-react": "^0.8.16",
    "fast-deep-equal": "^2.0.1",
    "grunt-strip-json-comments": "^2.2.0",
    "immutable": "^3.8.2",
    "react": "^16.2.0",
    "react-debounce-render": "^4.0.0",
    "react-dom": "^16.2.0",
    "react-json-tree": "^0.11.0",
    "react-lds": "^7.1.0",
    "react-split-pane": "^0.1.77",
    "react-virtualized": "^9.16.1",
    "scroll-into-view-if-needed": "^2.2.4"
  }
}
